{
  "name": "Light Professional",
  
  "dataColors": [
    "#0078D4",
    "#00A896",
    "#F77F00",
    "#9B59B6",
    "#E63946",
    "#2A9D8F",
    "#457B9D",
    "#E9C46A"
  ],
  
  "good": "#00A896",
  "neutral": "#8A8D90",
  "bad": "#E63946",
  
  "maximum": "#0078D4",
  "center": "#B8C0C8",
  "minimum": "#E8F1F8",
  
  "null": "#CCCCCC",
  
  "background": "#FFFFFF",
  "backgroundLight": "#FAFAFA",
  "backgroundNeutral": "#F5F5F5",
  
  "foreground": "#1A1A1A",
  "foregroundNeutralSecondary": "#616161",
  "foregroundNeutralTertiary": "#9E9E9E",
  
  "tableAccent": "#0078D4",
  
  "textClasses": {
    "label": {
      "fontSize": 10,
      "fontFace": "Segoe UI",
      "color": "#616161"
    },
    "title": {
      "fontSize": 12,
      "fontFace": "Segoe UI Semibold",
      "color": "#1A1A1A"
    },
    "header": {
      "fontSize": 14,
      "fontFace": "Segoe UI Semibold",
      "color": "#1A1A1A"
    },
    "callout": {
      "fontSize": 45,
      "fontFace": "Segoe UI Light",
      "color": "#1A1A1A"
    }
  },
  
  "visualStyles": {
    "page": {
      "*": {
        "background": [{
          "color": { "solid": { "color": "#FFFFFF" } },
          "transparency": 0
        }],
        "outspace": [{
          "color": { "solid": { "color": "#F5F5F5" } },
          "transparency": 0
        }]
      }
    },
    
    "card": {
      "*": {
        "labels": [{
          "fontSize": 45,
          "fontFamily": "Segoe UI Light",
          "color": { "solid": { "color": "#1A1A1A" } }
        }],
        "categoryLabels": [{
          "show": true,
          "fontSize": 11,
          "fontFamily": "Segoe UI",
          "color": { "solid": { "color": "#616161" } }
        }],
        "background": [{
          "show": true,
          "color": { "solid": { "color": "#FAFAFA" } },
          "transparency": 0
        }],
        "visualBorder": [{
          "show": true,
          "color": { "solid": { "color": "#E0E0E0" } },
          "radius": 4,
          "width": 1
        }],
        "shadow": [{
          "show": true,
          "color": { "solid": { "color": "#000000" } },
          "transparency": 90,
          "position": "Outer",
          "angle": 45,
          "distance": 5,
          "blur": 8
        }]
      }
    },
    
    "tableEx": {
      "*": {
        "grid": [{
          "gridVerticalColor": { "solid": { "color": "#E8E8E8" } },
          "gridHorizontalColor": { "solid": { "color": "#E8E8E8" } },
          "textSize": 10,
          "fontFamily": "Segoe UI",
          "rowPadding": 4
        }],
        "columnHeaders": [{
          "fontColor": { "solid": { "color": "#1A1A1A" } },
          "backColor": { "solid": { "color": "#F5F5F5" } },
          "fontSize": 11,
          "fontFamily": "Segoe UI Semibold",
          "outline": "BottomOnly"
        }],
        "values": [{
          "fontColorPrimary": { "solid": { "color": "#1A1A1A" } },
          "backColorPrimary": { "solid": { "color": "#FFFFFF" } },
          "fontColorSecondary": { "solid": { "color": "#1A1A1A" } },
          "backColorSecondary": { "solid": { "color": "#FAFAFA" } },
          "fontSize": 10,
          "fontFamily": "Segoe UI"
        }],
        "total": [{
          "fontColor": { "solid": { "color": "#FFFFFF" } },
          "backColor": { "solid": { "color": "#0078D4" } },
          "fontSize": 11,
          "fontFamily": "Segoe UI Semibold",
          "bold": true
        }]
      }
    },
    
    "slicer": {
      "*": {
        "general": [{
          "outlineColor": { "solid": { "color": "#0078D4" } },
          "outlineWeight": 2
        }],
        "header": [{
          "show": true,
          "fontColor": { "solid": { "color": "#1A1A1A" } },
          "background": { "solid": { "color": "#F5F5F5" } },
          "outline": "BottomOnly",
          "textSize": 12,
          "fontFamily": "Segoe UI Semibold"
        }],
        "items": [{
          "fontColor": { "solid": { "color": "#1A1A1A" } },
          "background": { "solid": { "color": "#FFFFFF" } },
          "textSize": 10,
          "fontFamily": "Segoe UI"
        }],
        "slider": [{
          "color": { "solid": { "color": "#0078D4" } }
        }],
        "input": [{
          "fontColor": { "solid": { "color": "#1A1A1A" } }
        }]
      }
    },
    
    "clusteredBarChart": {
      "*": {
        "dataLabels": [{
          "show": false,
          "fontSize": 10,
          "fontFamily": "Segoe UI",
          "color": { "solid": { "color": "#1A1A1A" } }
        }],
        "categoryAxis": [{
          "fontSize": 10,
          "fontFamily": "Segoe UI",
          "fontColor": { "solid": { "color": "#616161" } }
        }],
        "valueAxis": [{
          "fontSize": 10,
          "fontFamily": "Segoe UI",
          "fontColor": { "solid": { "color": "#616161" } },
          "gridlineColor": { "solid": { "color": "#E8E8E8" } },
          "gridlineThickness": 1
        }]
      }
    }
  }
}
